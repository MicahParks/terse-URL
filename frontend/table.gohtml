<html lang="en-US">
<head>
    <title>Side Navigation Menu</title>
    <meta charset="utf-8">
    <link href="search.css" type="text/css" rel="stylesheet"/>
    <link href="shared.css" type="text/css" rel="stylesheet"/>
    <link href="table.css" media="screen" type="text/css" rel="stylesheet"/>
    <script src="table.js"></script>
</head>
<body>
<div class="searchContainer">
    <input type="text" placeholder="Search...">
    <div class="search"></div>
</div>
<table class="container" id="tta">
    <thead>
    <tr>
        <th><h1 onclick="sortTable(0)">Shortened</h1></th>
        <th><h1 onclick="sortTable(1)">Original</h1></th>
        <th><h1 onclick="sortTable(2)">Visits</h1></th>
        <th><h1 onclick="sortTable(3)">Edit</h1></th>
    </tr>
    </thead>
    <tbody>
    {{range .}}
        <tr>
            <td>{{.Shortened}}</td>
            <td>{{.Original}}</td>
            <td>{{.Visits}}</td>
            <td>Fake</td>
        </tr>
    {{end}}
    </tbody>
</table>
</body>
</html>
